# Copy to .env and fill with real secrets (never commit .env)
DATABASE_URL=postgresql://USER:PASS@HOST:5432/DB?sslmode=require
JWT_SECRET=change_me_32bytes_min
ENCRYPTION_KEY=32+_byte_random_key_here
AI_SERVICE_URL=http://localhost:5001
CORS_ORIGIN=http://localhost:3000

# When running locally or in testing environments without IAP, you can bypass
# subscription checks entirely and allow manual login/registration by setting
# these flags to "true".  In production, omit these or set them to "false".
SUBSCRIPTION_BYPASS=false
ALLOW_MANUAL_LOGIN=false
