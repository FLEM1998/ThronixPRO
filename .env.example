# Required environment variables for ThronixPRO
NODE_ENV=production
LOG_LEVEL=info

# Database
DATABASE_URL=postgresql://USER:PASSWORD@HOST:5432/DBNAME

# Auth / Security
JWT_SECRET=change_me_to_a_long_random_string
ENCRYPTION_KEY=change_me_to_32_bytes_key

# AI + Email services
AI_SERVICE_URL=
RESEND_API_KEY=

# IAP endpoints (override if using sandboxes)
HUAWEI_IAP_URL=https://iap-api.cloud.huawei.com
SAMSUNG_IAP_URL=https://iap.samsungapps.com
